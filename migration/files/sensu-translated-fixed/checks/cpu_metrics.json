{
  "type":"Check",
  "spec":{
    "name":"cpu_metrics",
    "type":"metric",
    "command":"metrics-cpu-pcnt-usage.rb",
    "interval":10,
    "subscriptions":[
      "localhost"
    ],
    "publish":true,
    "organization":"default",
    "environment":"default",
    "output_metric_format": "graphite_plaintext",
    "output_metric_handlers": [
      "influx-tcp"
    ]

  }
}
